@model StarWars.ViewModel.SwapiViewModel
@{
	Layout = null;
}
<h2>Personaje Buscado </h2>
<h5>CARACTERISTICAS</h5>
<p>NOMBRE: @Model.NamePeople</p>
<p>COLOR PELO: @Model.HairColor</p>

<h5>CARACTERISTICAS DEL PLANETA</h5>
<p>NOMBRE PLANETA: @Model.NamePlanet</p>
<p>TERRENO DEL PLANETA: @Model.TerrainPlanet</p>

<h5>DETALLES DE LA PELICULA</h5>
@foreach(var f in Model.listFilms)
{
	<p>Episodio: @f.Episode_Id </p>
	<p>Titulo: @f.Title</p>
	<p>Opening: @f.Opening_Crawl</p>
}
<h5>Detalles de la nave</h5>
@foreach(var s in Model.ListStarship)
{

	<p>Nombre Vehiculo : @s.Name</p>
}

